<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net35;net40</TargetFrameworks>
    <RootNamespace>NStandard</RootNamespace>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Authors>zmjack</Authors>
    <Company>dawnx.net</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.2.2.1</Version>
    <Description>DotNet Core extensions for system library.</Description>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <RepositoryUrl>https://github.com/zmjack/NStandard</RepositoryUrl>
    <PackageProjectUrl>https://github.com/zmjack/NStandard</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>Copyright © dawnx.net 2020</Copyright>
    <LangVersion>8.0</LangVersion>
    <PackageTags>System Standard Extension</PackageTags>
    <PackageIcon>_icon.png</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web" Condition="'$(TargetFramework)'!='netstandard2.0'" />
  </ItemGroup>

</Project>
